{
  "name": "badger-maths",
  "version": "0.0.1",
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "main": "dist/my-test-library.umd.cjs",
  "module": "dist/my-test-library.js",
  "exports": {
    ".": {
      "require": "./dist/my-test-library.umd.cjs",
